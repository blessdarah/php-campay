{
    "name": "blessdarah/php-campay",
    "description": "PHP package wrapper for campay momo integration",
    "autoload": {
        "psr-4": {
            "BlessDarah\\PhpCampay\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Bless Darah",
            "email": "blessdarahuba@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "nesbot/carbon": "^2.62",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "pestphp/pest": "^1.22"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
